<script src="/socket.io/socket.io.js"></script>
<script>
var socket = io.connect('http://localhost:8000/controller');
socket.on('connect', function () {
    chat.emit('hi!');
  });
function start(){
  socket.emit('feedcontrol', 'start');
  console.log('start feed');
}
function stop(){
  socket.emit('feedcontrol', 'stop');
}
</script>
<button onClick="start()">Start feed</button>
<button onClick="stop()">Stop Feed</button>
